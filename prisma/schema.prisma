// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Rows {
  id        String    @id @default(uuid())
  orderPad  String
  queue     Int?
  shortRow  Int?
  mediumRow Int?
  LongRow   Int?
  createdAt DateTime
  used      Boolean   @default(false)
  usedAt    DateTime?
}
